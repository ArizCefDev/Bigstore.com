@model DTO.EntityDTO.ContactDTO

@{
    ViewData["Title"] = "ContactAdd";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div id="page-wrapper">
    <div class="main-page">
        <div class="forms">
            <h2 class="title1">Bizimlə Əlaqə Formu</h2>
            <div class="form-grids row widget-shadow" data-example-id="basic-forms">
                <div class="form-title">
                    <h4>Yeni Form :</h4>
                </div>
                <div class="form-body">
                    <form method="post">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Foto</label>
                            <input type="text" asp-for="ImageURL" class="form-control" id="exampleInputEmail1" placeholder="Məlumatı daxil edin">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Ünvan</label>
                            <input type="text" asp-for="Address" class="form-control" id="exampleInputEmail1" placeholder="Məlumatı daxil edin">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Mətn</label>
                            <input type="text" asp-for="Text" class="form-control" id="exampleInputEmail1" placeholder="Məlumatı daxil edin">
                        </div>
                        
                        <div class="form-group">
                            <label for="exampleInputEmail1">Email</label>
                            <input type="email" asp-for="Mail" class="form-control" id="exampleInputEmail1" placeholder="Məlumatı daxil edin">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Telefon</label>
                            <input type="number" asp-for="Phone" class="form-control" id="exampleInputEmail1" placeholder="Məlumatı daxil edin">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Xəritə URL</label>
                            <input type="text" asp-for="Map" class="form-control" id="exampleInputEmail1" placeholder="Məlumatı daxil edin">
                        </div>

                        <button type="submit" class="btn btn-default">Əlavə et</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>